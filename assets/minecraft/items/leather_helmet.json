{
    "model": {
      "type": "minecraft:select",
      "property": "minecraft:custom_model_data",
      "cases": [
        {
          "when": "pk_waystones:gui_visibility_public",
          "model": {
            "type": "minecraft:model",
            "model": "waystones_rp:custom_gui/waystone/button/visibility_public"
          }
        },
        {
          "when": "pk_waystones:gui_visibility_private",
          "model": {
            "type": "minecraft:model",
            "model": "waystones_rp:custom_gui/waystone/button/visibility_private"
          }
        },
        {
          "when": "pk_waystones:gui_visibility_discover",
          "model": {
            "type": "minecraft:model",
            "model": "waystones_rp:custom_gui/waystone/button/visibility_discoverable"
          }
        }
      ],
      "fallback": {
        "type": "minecraft:model",
        "model": "minecraft:item/leather_helmet"
      }
    },
    "oversized_in_gui": true
  }